{
    "name": "rudra/redirect",
    "description": "Rudra framework",
    "type": "library",
    "keywords": [
        "rudra",
        "redirect",
        "redirection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Redirect/issues",    
        "source": "https://github.com/Jagepard/Rudra-Redirect"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/container": "self.version",
        "rudra/docs": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Redirect\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Redirect\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
